msgid "Accessories"
msgstr "Стандартні"

#: ../data/MATE_GnoteApplet.server.in.in.h:2 ../data/gnote.desktop.in.in.h:1
msgid "Gnote"
msgstr "Gnote"

#: ../data/MATE_GnoteApplet.server.in.in.h:3
msgid "Gnote Applet Factory"
msgstr "Фабрика аплетів Gnote"

#: ../data/MATE_GnoteApplet.server.in.in.h:4
msgid "Simple and easy to use note-taking"
msgstr "Проста та зручна програма для створення приміток"

#: ../data/MATE_GnoteApplet.xml.h:1 ../src/actionmanager.cpp:217
msgid "_About"
msgstr "_Про програму"

#: ../data/MATE_GnoteApplet.xml.h:2 ../src/actionmanager.cpp:208
#: ../src/tray.cpp:468
msgid "_Help"
msgstr "_Довідка"

#: ../data/MATE_GnoteApplet.xml.h:3 ../src/actionmanager.cpp:205
