;; -*- scheme -*-

(define-boxed MateModuleInfo
  (in-module "Mate")
  (c-name MateModuleInfo)
  (gtype-id "MATE_TYPE_MODULE_INFO")
  (copy-func "mate_module_info_copy")
  (release-func "mate_module_info_free")
  (fields
   '("const-gchar*" "name")
   '("const-gchar*" "version")
   '("const-gchar*" "description")
  )
)
