msgstr "*"

#: C/keyboard.page:23(p)
msgid "÷"
msgstr "÷"

#: C/keyboard.page:24(key)
msgid "/"
msgstr "/"

#: C/keyboard.page:27(p)
msgid "^"
msgstr "^"

#: C/keyboard.page:28(p)
msgid "<key>*</key> twice"
msgstr "<key>*</key> zweimal"

#: C/keyboard.page:31(p)
msgid "√"
msgstr "√"

#: C/keyboard.page:32(key) C/keyboard.page:36(key)
